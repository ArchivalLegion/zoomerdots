ColorNormalTaskBarApp="rgb:1E/1F/29"

#  Textcolor for task buttons of regular windows
ColorNormalTaskBarAppText="rgb:EF/72/BB"

#  Textcolor for task buttons of the active window
ColorActiveTaskBarAppText="rgb:FF/52/30"

#  Background for task buttons of the active window
ColorActiveTaskBarApp="rgb:28/2A/36"

#  Background for task buttons of minimized windows
ColorMinimizedTaskBarApp="rgb:1E/1F/29"

#  Textcolor for task buttons of minimized windows
ColorMinimizedTaskBarAppText="rgb:EF/72/BB"

#-----Listbox-----

ColorListBox="rgb:1C/1B/20"
ColorListBoxText="rgb:FF/FF/FF"
